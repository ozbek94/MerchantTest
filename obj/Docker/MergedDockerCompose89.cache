name: dockercompose3155283842087634021
services:
  merchanttest.api:
    build:
      context: C:\Users\alper\Documents\GitHub\MerchantTest
      dockerfile: MerchantTest/Dockerfile
    container_name: merchanttest.api
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
    image: merchanttestapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
  merchanttest.cache:
    image: redis:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 6379
        published: "6379"
        protocol: tcp
    restart: always
  merchanttest.database:
    container_name: merchanttest.database
    environment:
      POSTGRES_DB: merchanttest
      POSTGRES_PASSWORD: postgres
      POSTGRES_USER: postgres
    image: postgres:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\alper\Documents\GitHub\MerchantTest\.containers\merchanttest-db
        target: /var/lib/postgresql/data
        bind:
          create_host_path: true
  mongodb_container:
    image: mongo:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 27017
        published: "27017"
        protocol: tcp
    volumes:
      - type: volume
        source: mongodb_data_container
        target: /data/db
        volume: {}
networks:
  default:
    name: dockercompose3155283842087634021_default
volumes:
  mongodb_data_container:
    name: dockercompose3155283842087634021_mongodb_data_container